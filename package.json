{
  "name": "react-native-map-link",
  "version": "2.7.24",
  "description": "Open the map app of the user's choice with a specific location",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/includable/react-native-map-link.git"
  },
  "scripts": {
    "lint:ts": "tsc --noEmit",
    "lint:src": "eslint src --ext .js,.ts,.tsx",
    "lint:tests": "eslint tests --ext .js,.ts,.tsx",
    "lint": "npm run lint:ts && npm run lint:src && npm run lint:tests",
    "test": "jest",
    "build": "tsc && cp -nfr src/images dist/src/images",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react-native",
    "react-component",
    "maps",
    "link",
    "linking",
    "react",
    "ios",
    "android"
  ],
  "author": "Thomas Schoffelen <thomas@includable.com> (https://includable.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/includable/react-native-map-link/issues"
  },
  "homepage": "https://github.com/includable/react-native-map-link#readme",
  "dependencies": {
    "react-native-modal": "^10.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.40.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^26.0.22",
    "@types/react-native": "^0.64.2",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.61.0",
    "react-native": "^0.63.4",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./tests/setup.ts"
    ]
  }
}
